    "event",
    "delegation",
    "behavior",
    "keyboard"
  ],
  "dependencies": {
    "element-closest": "^2.0.1",
    "keyboardevent-key-polyfill": "^1.0.2",
    "matches-selector": "^1.0.0",
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-plugin-github": "^0.24.1",
    "jsdom": "^11.7.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.2.2",
    "prettier": "^1.11.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  }
